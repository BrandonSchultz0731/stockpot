{
  "$schema": "https://schema.railpack.com",
  "provider": "node",
  "packages": {
    "node": "22"
  },
  "steps": {
    "install": {
      "commands": ["...", "npm i -g pnpm && cd server && pnpm install --frozen-lockfile"]
    },
    "build": {
      "commands": ["...", "cd server && pnpm build"]
    }
  }
}
